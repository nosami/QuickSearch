<?xml version="1.0" encoding="UTF-8"?>
<ExtensionModel>
	<Runtime>
	</Runtime>
	<Extension path = "/MonoDevelop/Ide/Commands">
		<Category _name="QuickSearch" id="QuickSearch">
			<Command id="QuickSearch.QuickSearch"
			         _label = "QuickJump"
			         _description="Launch QuickSearch"
			         macShortcut="Alt|F"
			         defaultHandler="QuickSearch.QuickSearchHandler" />
		</Category>
	</Extension>
	<Extension path="/MonoDevelop/Ide/Pads">
        <Pad id="QuickSearch.QuickSearchPad" defaultPlacement="Bottom" _label="Quick Search" icon="md-magnify" class="QuickSearch.QuickSearchPad" />
    </Extension>
</ExtensionModel>
